<h1>Listing exams</h1>

<table border="1" style="border-left:1px;">
  <tr>
    <th>ID</th>
    <th>Subject</th>
    <th>Year</th>
    <th>Number</th>
    <th>Q/A</th>
    <th>File_name</th>
    <th>Content_type</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @exams.each do |exam| %>
  <tr>
    <td><%= exam.id %></td>
    <td><%= exam.subject.screen_title %></td>
    <td><%= exam.year %></td>
    <td><%= exam.number %></td>
    <td><%= exam.q_a.upcase %></td>
    <td><%= exam.file_name %></td>
    <td><%= exam.content_type %></td>
    <td><%= link_to 'Show', exam %></td>
    <td><%= link_to 'Edit', edit_exam_path(exam) %></td>
    <td><%= link_to 'Destroy', exam, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<% @exams.each do |exam| %>
  <% p exam.number %>
  <% p exam.q_a %>
<% end %>
<%= link_to 'New Exam', new_exam_path %>

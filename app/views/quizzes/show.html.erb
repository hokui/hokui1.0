<p id="notice"><%= notice %></p>

<p>
  <b>ID:</b>
  <%= @quiz.id %>
</p>

<p>
  <b>Subject:</b>
  <%= @quiz.subject.screen_title %>
</p>

<p>
  <b>Number:</b>
  <%= @quiz.number %>
</p>

<p>
  <b>Q/A:</b>
  <%= if @quiz.type==1 do "A" else "Q" end %>
</p>

<p>
  <b>File:</b>
  <%= @quiz.file %>
</p>

<p>
  <b>File_name:</b>
  <%= @quiz.file_name %>
</p>

<p>
  <b>Content_type:</b>
  <%= @quiz.content_type %>
</p>

<%= link_to 'Edit', edit_quiz_path(@quiz) %> |
<%= link_to 'Back', quizzes_path %>
